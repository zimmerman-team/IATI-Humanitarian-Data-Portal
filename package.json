  "scripts": {
    "start": "REACT_APP_PROD=false node scripts/start.js",
    "start:production": "REACT_APP_PROD=true node scripts/start.js",
    "build": "REACT_APP_PROD=false node scripts/build.js",
    "build:production": "REACT_APP_PROD=true node scripts/build.js",
    "test": "REACT_APP_PROD=false node scripts/test.js",
    "storybook": "start-storybook -p 9009 -s public --ci",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "check-types": "tsc",
    "cy:ci-start": "start-server-and-test start http://localhost:3000 cy:run:record:parallel",
    "cy:run": "cypress run",
    "cy:run:record:parallel": "cypress run --record --key 9f5380f4-1876-4758-aded-8e30971fb7b6 --parallel",
    "cy:open": "cypress open",
    "analyse": "madge src/index.tsx",
    "depcheck": "depcheck",
    "why": "whybundled build/stats.json --duplicatesOnly",
    "mon": "nodemon"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "echo pre && echo commit && echo police",
      "pre-push": "echo pre push police",
      "post-merge": "yarn install"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "lighthouse": {
    "requiredScores": {
      "performance": 95,
      "accessibility": 90,
      "best-practices": 100,
      "seo": 90
    }
  }

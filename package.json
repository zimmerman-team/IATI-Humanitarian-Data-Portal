    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "storybook": "start-storybook -p 9009 -s public --ci",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "check-types": "tsc",
    "cy:run": "cypress run",
    "cy:run:record:parallel": "cypress run --record --key 9f5380f4-1876-4758-aded-8e30971fb7b6 --parallel",
    "cy:run:record:percy:parallel:reporter": "percy exec -- cypress run --record --key 9f5380f4-1876-4758-aded-8e30971fb7b6 --parallel --reporter junit",
    "cy:open": "cypress open",
    "analyse": "madge src/index.tsx",
    "depcheck": "depcheck",
    "why": "whybundled build/stats.json --duplicatesOnly ",
    "postbuild": "react-snap",
    "version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
    "release": "auto changelog && npm run version && npm run publish && auto release",
    "change": "beachball change",
    "check": "beachball check",
    "beachball:publish": "beachball publish"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "echo pre && echo commit && echo police",
      "pre-push": "echo pre push police"
    }
  },
  "lighthouse": {
    "requiredScores": {
      "performance": 95,
      "accessibility": 90,
      "best-practices": 100,
      "seo": 90
    }
  }
}
